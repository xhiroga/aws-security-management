{
  "name": "baseline",
  "version": "0.1.0",
  "bin": {
    "baseline-dynamic": "bin/baseline-dynamic.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "lint": "run-p -l -c --aggregate-output lint:*",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "fix": "run-s fix:prettier fix:eslint",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "fix:prettier": "npm run lint:prettier -- --write",
    "test": "run-p -l -c --aggregate-output lint:* unit",
    "unit": "jest",
    "cdk": "cdk"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.122.0",
    "@types/jest": "^26.0.10",
    "@types/node": "10.17.27",
    "@types/source-map-support": "^0.5.4",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "aws-cdk": "1.122.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^24.4.0",
    "jest": "^26.4.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.0",
    "ts-jest": "^26.2.0",
    "ts-node": "^9.0.0",
    "typescript": "~3.9.7"
  },
  "dependencies": {
    "@aws-cdk/aws-ce": "^1.122.0",
    "@aws-cdk/aws-cloudwatch": "^1.124.0",
    "@aws-cdk/aws-sns": "^1.122.0",
    "@aws-cdk/core": "1.122.0",
    "@aws-sdk/client-cloudwatch-logs": "^3.33.0",
    "@aws-sdk/client-ec2": "^3.32.0",
    "source-map-support": "^0.5.16"
  }
}
